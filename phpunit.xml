<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="phpunit-bootstrap.php" colors="true">
    <testsuites>
        <testsuite name="PHP_CodeSniffer Test Suite">
            <directory suffix="UnitTest.php">VariableAnalysis/Tests/VariableAnalysis</directory>
        </testsuite>
    </testsuites>
    <php>
        <env name="PHPCS_IGNORE_TESTS" value="PHPCompatibility,Generic,MySource,PEAR,PHPCS,PSR1,PSR2,PSR12,Squiz,Zend"/>
    </php>
</phpunit>
